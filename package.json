{
  "name": "jampass",
  "version": "0.0.2-edge.2.7",
  "description": "A static site generator",
  "main": "src/jampass.js",
  "type": "module",
  "bin": {
    "jampass": "src/cli.js"
  },
  "scripts": {
    "lint": "eslint src",
    "serve": "node src/cli serve -s demos/site",
    "watch": "node src/cli watch -s demos/site",
    ":lint": "node src/cli lint -s demos/site",
    "start": "node src/cli -s demos/site",
    "debug": "node --inspect src/cli watch -s demos/site -D"
  },
  "keywords": [
    "SSG",
    "static",
    "site",
    "generator"
  ],
  "author": "Simao Nziaka",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.14.8",
    "@babel/eslint-parser": "^7.14.7",
    "autoprefixer": "^10.4.2",
    "browser-sync": "^2.27.7",
    "browserify": "^17.0.0",
    "cheerio": "^1.0.0-rc.10",
    "chokidar": "^3.5.2",
    "colorette": "^2.0.16",
    "commander": "^8.3.0",
    "consola": "^2.15.3",
    "consolidate": "^0.16.0",
    "cssnano": "^5.0.6",
    "debug": "^4.3.3",
    "del": "^6.0.0",
    "dotenv": "^10.0.0",
    "esbuild": "^0.14.22",
    "eslint": "^7.29.0",
    "handlebars": "^4.7.7",
    "html-validate": "^6.4.0",
    "marky": "^1.2.2",
    "minify": "^8.0.3",
    "postcss": "^8.3.5",
    "postcss-hash": "^3.0.0",
    "postcss-preset-env": "^7.2.3",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "faker": "^5.5.3",
    "trie-search": "^1.3.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akaizn-junior/jampass.git"
  },
  "bugs": {
    "url": "https://github.com/akaizn-junior/jampass/issues"

{
  "name": "jampass",
  "version": "0.1.3",
  "description": "A static web builder",
  "main": "src/core.js",
  "type": "module",
  "bin": {
    "jampass": "src/cli.js"
  },
  "scripts": {
    "lint": "eslint src",
    "serve": "node --trace-warnings src/cli serve .",
    "watch": "node --trace-warnings src/cli watch .",
    ":lint": "node --trace-warnings src/cli lint .",
    "start": "node --trace-warnings src/cli . --env production"
  },
  "keywords": [
    "SSG",
    "static",
    "site",
    "generator",
    "jamstack",
    "web"
  ],
  "author": "Simao Nziaka",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.14.8",
    "@babel/eslint-parser": "^7.14.7",
    "autoprefixer": "^10.4.2",
    "browser-sync": "^2.27.7",
    "cheerio": "^1.0.0-rc.10",
    "chokidar": "^3.5.2",
    "colorette": "^2.0.16",
    "commander": "^8.3.0",
    "consola": "^2.15.3",
    "consolidate": "^0.16.0",
    "cssnano": "^5.0.6",
    "debug": "^4.3.3",
    "del": "^6.0.0",
    "dotenv": "^10.0.0",
    "esbuild": "^0.14.22",
    "eslint": "^7.29.0",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.7",
    "html-minifier-terser": "^7.0.0-alpha.1",
    "html-validate": "^6.4.0",
    "marked": "^4.0.14",
    "marky": "^1.2.2",
    "postcss": "^8.3.5",
    "postcss-hash": "^3.0.0",
    "postcss-preset-env": "^7.2.3",
    "postcss-sass": "^0.5.0",
    "postcss-scss": "^4.0.3",
    "postcss-url": "^10.1.3",
    "sanitize-html": "^2.7.0",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "faker": "^5.5.3",
    "trie-search": "^1.3.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akaizn-junior/jampass.git"
  },
